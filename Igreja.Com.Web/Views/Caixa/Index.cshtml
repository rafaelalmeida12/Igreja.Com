
<h4>Entradas</h4>
<div class="row">
    <div class="col-lg-3 col-md-6 col-sm-6">
        <a asp-action="Create" asp-controller="Dizimos">
            <div class="card card-stats">
                <div class="card-header card-header-success card-header-icon">
                    <div class="card-icon">
                        <i class="material-icons">add</i>
                    </div>
                    <h3 class="card-title">
                        Dizimos
                    </h3>
                </div>
            </div>
        </a>
    </div>
    <div class="col-lg-3 col-md-6 col-sm-6">
        <a asp-action="Create" asp-controller="Ofertas">
            <div class="card card-stats">
                <div class="card-header card-header-success card-header-icon">
                    <div class="card-icon">
                        <i class="material-icons">add</i>
                    </div>
                    <h3 class="card-title">
                        Ofertas
                    </h3>
                </div>
            </div>
        </a>
    </div>

    <h4>Saida</h4>

    <div class="col-lg-3 col-md-6 col-sm-6">
        <a asp-action="Index" asp-controller="Despesa">
            <div class="card card-stats">
                <div class="card-header card-header-danger card-header-icon">
                    <div class="card-icon">
                        <i class="material-icons">remove</i>
                    </div>
                    <h3 class="card-title">
                        Despesas
                    </h3>
                </div>
            </div>
        </a>
    </div>
</div>
<h4>Movimentações</h4>
<div class="row">
    <div class="col-lg-3 col-md-6 col-sm-6">
        <a asp-action="Index" asp-controller="Movimentacao">
            <div class="card card-stats">
                <div class="card-header card-header-danger card-header-icon">
                    <div class="card-icon">
                        <i class="material-icons">remove</i>
                    </div>
                    <h3 class="card-title">
                        Movimentações
                    </h3>
                </div>
            </div>
        </a>
    </div>
</div>
<div class="row">
    <h2>Entrada: @ViewBag.Entradas</h2> <br />
    <h2>Saida: @ViewBag.Saidas</h2>
</div>

